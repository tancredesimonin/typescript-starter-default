{
  "name": "typescript-starter-default",
  "version": "1.0.0",
  "description": "Fully configured environment for Typescript projects, still not opinionated.",
  "main": "./build/index.js",
  "scripts": {
    "prettier": "prettier  \"./src/**/*.{ts,tsx} \"",
    "prettier:fix": "prettier --write  \"./src/**/*.{ts,tsx} \"",
    "lint": "eslint  \"./src/**/*.{ts,tsx} \"",
    "lint:fix": "eslint --fix  \"./src/**/*.{ts,tsx} \"",
    "prepare": "husky install",
    "dev": "ts-node-dev --respawn -- src/",
    "build": "tsc",
    "js": "node ./build/index.js"
  },
  "keywords": [
    "typescript",
    "boilerplate",
    "starter",
    "vscode"
  ],
  "author": "Tancr√®de Simonin",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  }
}
